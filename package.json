{
  "name": "gavel2html",
  "version": "0.0.0-semantically-released",
  "description": "Render Gavel.js output to HTML diff",
  "main": "lib/index.js",
  "scripts": {
    "build": "coffee -b -c -o lib/ src/",
    "lint": "coffeelint src",
    "test": "npm run test:server && npm run test:browser",
    "test:server": "mocha \"test/**/*.coffee\"",
    "test:browser": "mochify \"test/**/*.coffee\" --transform=coffeeify --extension=.coffee",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiaryio/gavel2html.git"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "googlediff": "^0.1.0",
    "is-type": "~0.0.1",
    "json-pointer": "^0.6.0",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.12.5",
    "coffeeify": "^2.1.0",
    "coffeelint": "^1.16.0",
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.3.0",
    "mochify": "^3.1.1",
    "semantic-release": "^6.3.2"
  },
  "keywords": [
    "html",
    "http",
    "validation",
    "diff",
    "request",
    "response",
    "gavel"
  ],
  "author": "Apiary Czech Republic, s.r.o. <support@apiary.io>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
